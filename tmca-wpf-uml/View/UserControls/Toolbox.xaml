<UserControl x:Class="tmca_wpf_uml.View.UserControls.Toolbox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:tmca_wpf_uml.View.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <StackPanel>
        <Button Content="New Class" Command="{Binding AddClassCommand}"/>
        <Button Content="New Association" Command="{Binding AddAssociationCommand}"/>
        <Button Content="New TextBox" Command="{Binding AddTextBoxCommand}"/>
    </StackPanel>
</UserControl>
